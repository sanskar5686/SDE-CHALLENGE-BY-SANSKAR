// 1st approach 
Loop -
i = 0 to n-1 
j = 0 to m-1
// Idea: Just do array[j][n-i-1] = array[i][j] 
#include <bits/stdc++.h> 

vector<vector<int>> rotateMatrix(vector<vector<int>> &mat, int n, int m)
{
    // Write your code here
    vector<vector<int>> ans(n,vector<int> (n,0));
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            mat[j][n-i-1] =  mat[i][j];
        }
    }
    return ans;
}


// 2nd approach 
Loop - 
i= 0 to n-1
j= 0 to m-1
// Idea:  S1-> swap(matrix[i][j], matrix[j][i]);
          S2->  //reversing each row of the matrix
  for (int i = 0; i < n; i++) {
    reverse(matrix[i].begin(), matrix[i].end());
  }
}

